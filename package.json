{
  "name": "postman-aws_lambda",
  "version": "1.0.0",
  "description": "A lambda to export extra metrics for RDS",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel-changed",
    "start": "node dist/index.js",
    "type-check": "tsc -p jsconfig.json",
    "zip": "yarn && yarn build && yarn install --production && zip -r -u function.zip .",
    "lint": "eslint src",
    "lint:fix": "yarn lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohit-smpx/postman-aws_lambda.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "rds",
    "metrics",
    "monitoring"
  ],
  "author": "Rohit Gohri <gohri.rohit@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rohit-smpx/postman-aws_lambda/issues"
  },
  "homepage": "https://github.com/rohit-smpx/postman-aws_lambda#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-bigint": "^7.4.4",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@smpx/babel-changed": "^0.2.2",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "source-map-support": "^0.5.13",
    "typescript": "^3.6.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "modules": "commonjs",
          "shippedProposals": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-bigint",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "@smpx/cfg": "^1.2.1",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.556.0",
    "got": "^9.6.0",
    "knex": "^0.19.5",
    "lodash": "^4.17.15",
    "mysql": "^2.17.1"
  }
}
